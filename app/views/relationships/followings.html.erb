<h1>Follow Users</h1>
<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <% @relationships.each do |relationship| %>
        <td><%= image_tag @relationships.user.get_profile_image, size: '50x50' %></td>
        <td><%= @relationship.user.name %></td>
        <td>フォロー数：<%= @relationships.user.followings.count %></td>
        <td>フォロワー数：<%= @relationshipuser.followers.count %></td>
        <td><%= link_to 'Show',user_path(@user) %></td>
        <% end %>
      </tr>
  </tbody>
</table>
